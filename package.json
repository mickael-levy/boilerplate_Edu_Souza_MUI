{
  "name": "boilerplate_edu_souza",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "prepare": "husky install",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "lint": "eslint src --max-warnings=0",
    "test": "jest --passWithNoTests",
    "test:watch": "yarn test --watch",
    "test:staged": "yarn test --bail --findRelatedTests",
    "test:ci": "yarn test --bail --coverage",
    "test:watchFull": "yarn test --bail --coverage --findRelatedTests",
    "storybook": "start-storybook -s ./public -p 6006",
    "storybook:cleancache": "start-storybook -s ./public -p 6006 --no-manager-cache",
    "build-storybook": "build-storybook -s ./public",
    "generate:component": "yarn plop --plopfile ./generators/component/plopfile.js",
    "commit": "git commit",
    "commit:all": "git add --all && git commit -a",
    "clean-install": "rm -rf node_modules/ && yarn cache clean && yarn install --pure-lockfile",
    "upgrade:dep": "yarn upgrade-interactive --latest"
  },
  "lint-staged": {
    "src/**/*": [
      "yarn lint --fix",
      "yarn test:staged"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.12.1",
    "@material-ui/icons": "^4.11.2",
    "next": "11.0.1",
    "next-pwa": "^5.2.23",
    "polished": "^4.1.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-slick": "^0.28.1",
    "styled-components": "^5.3.0",
    "styled-icons": "^10.35.0",
    "styled-media-query": "^2.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-typescript": "^7.14.5",
    "@next/eslint-plugin-next": "^11.0.1",
    "@storybook/addon-actions": "^6.3.3",
    "@storybook/addon-essentials": "^6.3.3",
    "@storybook/addon-links": "^6.3.3",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-viewport": "^6.3.3",
    "@storybook/react": "^6.3.3",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.1.9",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.0.1",
    "@types/react": "^17.0.14",
    "@types/react-slick": "^0.23.5",
    "@types/styled-components": "^5.1.11",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.13.1",
    "eslint": "^7.30.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "jest-styled-components": "^7.0.4",
    "lint-staged": "^11.0.0",
    "plop": "^2.7.4",
    "prettier": "2.3.2",
    "typescript": "^4.3.5"
  }
}
